{
  "input": {
    "path": "input",
    "filePattern": ".*\\.midi?$"
  },
  "output": {
    "path": "output",
    "fileName": "${name}-nrm.${ext}"
  },
  "velocities": {
    "min": 70.0,
    "max": 127.0,
    "scaleFactor": 1.0,
    "removeSilent": true
  },
  "channels": {
    "match": [
      {
        "patterns": [".*Pizzicato?.*"],
        "channel": 1,
        "exclusive": true
      },
      {
        "patterns": [".*Strings?.*"],
        "channel": 2
      },
      {
        "includes": [".*Harpsi.*", ".*Clavichord.*", ".*Carillon.*"],
        "channel": 3
      }
    ]
  }
}