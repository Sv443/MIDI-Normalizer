{
  "velocities": {
    "min": 70.0,
    "max": 127.0,
    "scaleFactor": 1.2
  },
  "channels": {
    "match": [
      {
        "patterns": [".*Pizzicato?.*"],
        "channels": [1],
        "exclusive": true
      },
      {
        "patterns": [".*Strings?.*"],
        "channels": [2]
      },
      {
        "includes": [".*Harpsi.*", ".*Clavichord.*", ".*Carillon.*"],
        "channels": [3, 4]
      }
    ]
  }
}