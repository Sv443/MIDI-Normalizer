{
  "input": {
    "directory": "input",
    "filePattern": ".*\\.midi?$",
    "patternFlags": "i"
  },
  "output": {
    "directory": "output",
    "fileName": "${name}-nrm.${ext}",
    "clearDirectory": true
  },
  "velocities": {
    "min": 90.0,
    "max": 127.0,
    "removeSilent": true
  },
  "channels": {
    "match": [
      {
        "patterns": [".*Pizzicato?.*"],
        "channel": 1,
        "exclusive": true
      },
      {
        "patterns": [".*Strings?.*"],
        "channel": 2
      },
      {
        "includes": [".*Harpsi.*", ".*Clavichord.*", ".*Carillon.*"],
        "channel": 3
      }
    ]
  }
}